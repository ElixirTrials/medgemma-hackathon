# Copy to .env and set your values. Do not commit .env.

# UMLS (required for UMLS MCP server and integration tests)
# Get a key at https://uts.nlm.nih.gov/uts/signup-login
UMLS_API_KEY=

# Optional UMLS overrides
# UMLS_BASE_URL=https://uts-ws.nlm.nih.gov/rest
# UMLS_CACHE_DIR=
# UMLS_TIMEOUT_SECONDS=10

# Vertex AI Model Garden (required for MedGemma agentic grounding)
MODEL_BACKEND=vertex
GCP_PROJECT_ID=
GCP_REGION=europe-west4
VERTEX_ENDPOINT_ID=

# GCloud configuration profile (activated by make run-dev / run-api)
GCLOUD_PROFILE=

# MLflow tracking (required for experiment tracking and observability)
# When running locally via "make run-dev", MLflow runs on localhost:5001
MLFLOW_TRACKING_URI=http://localhost:5001

# For Docker Compose and full stack, see infra/.env.example
# (DATABASE_URL, API_PORT, GCS_BUCKET_NAME, etc.)
